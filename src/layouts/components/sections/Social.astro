---
import SocialLinks from "@/components/SocialLinks.astro";
import SocialMediaPosts from "@/components/socialMediaPosts.astro";

const { social } = Astro.props;

const { links, posts } = social;
---

<section
  class="section px-10 font-montserrat flex justify-center"
  style="background-color: #f5f4fe;"
>
  <div class="inner-wrapper w-fit flex flex-col gap-10 max-w-[1000px]">
    <SocialLinks links={links}/>
    <SocialMediaPosts posts={posts} links={links} />
  </div>
</section>
